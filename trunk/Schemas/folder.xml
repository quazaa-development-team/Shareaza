<schemaDescriptor location="http://www.shareaza.com/schemas/folder.xsd" xmlns="http://www.shareaza.com/schemas/SchemaDescriptor.xsd">

       <object type="folder" availability="default"/>
       <titles>
               <title language="en">Folder</title>
               <title language="af">Gids</title>
               <title language="de">Ordner</title>
               <title language="es-ve">Carpeta</title>
               <title language="lt">Katalogas</title>
               <title language="nl">Map</title>
               <title language="no">Mappe</title>
       </titles>
       <images>
               <icon path="folder.ico"/>
       </images>
       <members>
               <member name="title" search="generic">
                       <title language="en">Title</title>
                       <title language="af">Titel</title>
                       <title language="de">Titel</title>
                       <title language="es-ve">Título</title>
                       <title language="lt">Pavadinimas</title>
                       <title language="nl">Titel</title>
                       <title language="no">Tittel</title>
               </member>
               <member name="description" search="none">
                       <title language="en">Description</title>
                       <title language="af">Beskrywing</title>
                       <title language="de">Beschreibung</title>
                       <title language="es-ve">Descripción</title>
                       <title language="lt">Aprašas</title>
                       <title language="nl">Omschrijving</title>
                       <title language="no">Beskrivelse</title>
               </member>
               <member name="collector" search="indexed">
                       <title language="en">Collector</title>
                       <title language="af">Versamelaar</title>
                       <title language="de">Sammler</title>
                       <title language="es-ve">Colector</title>
                       <title language="lt">Kolekcionierius</title>
                       <title language="nl">Samensteller</title>
                       <title language="no">Samler</title>
               </member>
               <member name="comments">
                       <title language="en">Comments</title>
                       <title language="af">Kommentaar</title>
                       <title language="de">Kommentar</title>
                       <title language="es-ve">Comentarios</title>
                       <title language="lt">Komentarai</title>
                       <title language="nl">Opmerkingen</title>
                       <title language="no">Kommentar</title>
               </member>
       </members>
       <contains>
               <object type="folder" location="http://www.shareaza.com/schemas/folder.xsd"/>
               <object type="file" location="http://www.shareaza.com/schemas/file.xsd"/>
       </contains>
       <headerContent version="1.0">
               <title>{title}</title>
               <subtitle>{description}</subtitle>
       </headerContent>

</schemaDescriptor>