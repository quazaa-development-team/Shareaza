<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://www.shareaza.com/schemas/Template.xsd" version="1.0">
<manifest	name="Template for Video"
			author="Rolandas Rudomanskis"
			version="1.0"
			link="http://www.shareaza.com/"
			email="contact@shareaza.com"
			description="The template for video collections. Enter titles, descriptions and times in metadata for each file."
			icon="video.ico"
/>
<files>
	<file type="main" path="VideoCollection.tpl"/>
	<file type="evenfile" path="EvenFile.tpl"/>
	<file type="oddfile" path="OddFile.tpl"/>
	<!-- <file path="other.tpl"/> -->
</files>
<images>
	<image path="images\bar_on.gif"/>
	<image path="images\bar_off.gif"/>
	<image path="images\Screenshot_Part.png"/>
	<image path="images\Top.png"/>
	<image path="images\res.gif"/>
	<image path="images\bg.png"/>
</images>
<wizardText>
	<text language="lt">
		<item id="1" type="textbox" value="Atveriant bylą" default="Atverti bylą"/>
		<item id="2" type="textbox" value="Bylai siunčiantis" default="Siunčiama"/>
		<item id="3" type="textbox" value="Atsisiuntimo veiksmas" default="Atsisiųsti dabar"/>
		<item id="4" type="textbox" value="Rinkinio aprašymas"/>
		<item id="5" type="textbox" value="Web resurso aprašymas"/>
		<item id="6" type="colorpicker" value="Nelyginių eilučių spalva" default="#FFFFFF"/>
		<item id="7" type="colorpicker" value="Lyginių eilučių spalva" default="#E9EDEF"/>
		<item id="8" type="textbox" value="Apie leidėjų grupę"/>
		<item id="9" type="multi-filepicker" value="Paveikslėlis prie bylos" default="images/Screenshot_Part.png"/>
	</text>
	<text language="ja">
		<item id="1" type="textbox" value="”開く”の表示" default="開く"/>
		<item id="2" type="textbox" value="ダウンロード中の表示" default="ダウンロード中"/>
		<item id="3" type="textbox" value="”ダウンロード”の表示" default="今すぐダウンロード"/>
		<item id="4" type="textbox" value="アルバム詳細"/>
		<item id="5" type="textbox" value="Webリソースの説明"/>
		<item id="6" type="colorpicker" value="遇数行の色" default="#FFFFFF"/>
		<item id="7" type="colorpicker" value="奇数行の色" default="#E9EDEF"/>
		<item id="8" type="textbox" value="リリースグループ詳細"/>
		<item id="9" type="multi-filepicker" value="スクリーンショット" default="images/Screenshot_Part.png"/>
	</text>
	<!-- It's better to place english translation at the end to exit data collection immediately when language specific data already loaded -->
	<text language="en">
		<item id="1" type="textbox" value="File opening action" default="Open File"/>
		<item id="2" type="textbox" value="While downloading" default="Downloading"/>
		<item id="3" type="textbox" value="Download action" default="Download Now"/>
		<item id="4" type="textbox" value="Album description"/>
		<item id="5" type="textbox" value="Description of Web Resource"/>
		<item id="6" type="colorpicker" value="Color for even rows" default="#FFFFFF"/>
		<item id="7" type="colorpicker" value="Color for odd rows" default="#E9EDEF"/>
		<item id="8" type="textbox" value="About the release group"/>
		<item id="9" type="multi-filepicker" value="Image for the file" default="images/Screenshot_Part.png"/>
	</text>
</wizardText>
</template>