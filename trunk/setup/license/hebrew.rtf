\pard\rtlpar\nowidctlpar\qr  \b 8.\lang1037\b0\f0\rtlch  \'e0\'ed \'e4\'f4\'f6\'fa \'e4\'fa\'e5\'eb\'f0\'e9\'fa \'e5/\'e0\'e5 \'e4\'f9\'e9\'ee\'e5\'f9 \'e1\'e4 \'ee\'e5\'e2\'e1\'ec\'e9\'ed \'e1\'ee\'e3\'e9\'f0\'e5\'fa \'ee\'f1\'e5\'e9\'ee\'e5\'fa, \'e1\'e9\'ef\lang1063\f1\ltrch  \lang1037\f0\rtlch\'e0\'ed \'f2\'ec \'e9\'e3\'e9 \'f4\'e8\'f0\'e8\'e9\'ed \'e5\'e1\'e9\'ef \'e0\'ed \'f2\'ec \'e9\'e3\'e9 \'e6\'eb\'e5\'e9\'e5\'fa \'e9\'e5\'f6\'f8\'e9\'ed, \'e1\'f2\'ec \'e6\'eb\'e5\'fa\lang1063\f1\ltrch  \lang1037\f0\rtlch\'e4\'e9\'e5\'f6\'f8\'e9\'ed \'e4\'ee\'f7\'e5\'f8\'e9, \'e4\'ee\'eb\'f4\'e9\'f3 \'e0\'fa \'e4\'fa\'e5\'eb\'f0\'e9\'fa \'ec\'f8\'e9\'f9\'e9\'e5\'ef \'e6\'e4, \'f8\'f9\'e0\'e9 \'ec\'e4\'e5\'f1\'e9\'f3\lang1063\f1\ltrch  \lang1037\f0\rtlch\'e4\'e2\'e1\'ec\'e4 \'ee\'f4\'e5\'f8\'f9\'fa \'f2\'ec \'e4\'f4\'f6\'e4 \'e1\'fa\'e7\'e5\'ed \'e2\'e9\'e0\'e5\'e2\'f8\'f4\'e9 \'e4\'ee\'e5\'f6\'e9\'e0\'e4 \'ee\'e4\'eb\'ec\'ec \'ee\'e3\'e9\'f0\'e5\'fa \'e0\'ec\'e4\lang1063\f1\ltrch , \lang1037\f0\rtlch\'e1\'e0\'e5\'f4\'ef \'f9\'e4\'e4\'f4\'f6\'e4 \'fa\'e4\'e9\'e4 \'ee\'e5\'fa\'f8\'fa \'f8\'f7 \'e1\'ee\'e3\'e9\'f0\'e5\'fa \'f9\'ec\'e0 \'e4\'e5\'f6\'e0\'e5 \'eb\'ea \'ee\'e4\'eb\'ec\'ec \'e0\'e5\lang1063\f1\ltrch  \lang1037\f0\rtlch\'e1\'e9\'f0\'e9\'e4\'ef. \'e1\'ee\'f7\'f8\'e4 \'eb\'e6\'e4, \'f8\'e9\'f9\'e9\'e5\'ef \'e6\'e4 \'ee\'f9\'ec\'e1 \'ec\'fa\'e5\'eb\'e5 \'e0\'fa \'e4\'ee\'e2\'e1\'ec\'e4 \'eb\'e0\'e9\'ec\'e5 \'f0\'eb\'fa\'e1\'e4\lang1063\f1\ltrch  \lang1037\f0\rtlch\'e1\'e2\'e5\'f3 \'f8\'e9\'f9\'e9\'e5\'ef \'e6\'e4\lang1063\f1\ltrch . \par
