\lang1037\f0\rtlch\'f1\'f2\'e9\'f3 \'e6\'e4 \'e0\'e9\'f0\'e5 \'ee\'e9\'e5\'f2\'e3 \'ec\'e2\'f8\'e5\'ed \'ec\'ea \'ec\'e4\'f4\'f8 \'f4\'e8\'f0\'e8\'e9\'ed \'eb\'ec\'f9\'e4\'ed \'e0\'e5 \'fa\'e1\'e9\'f2\'e5\'fa \'e6\'eb\'e5\'fa\lang1063\f1\ltrch  \lang1037\f0\rtlch\'f7\'f0\'e9\'e9\'f0\'e9\'e5\'fa \'e0\'e7\'f8\'e5\'fa \'eb\'ec\'f9\'e4\'ef \'e0\'e5 \'ec\'f2\'f8\'f2\'f8 \'f2\'ec \'fa\'f7\'f4\'e5\'fa\'e4 \'f9\'ec \'fa\'e1\'e9\'f2\'fa \'e6\'eb\'e5\'fa \'ee\'f2\'e9\'ef\lang1063\f1\ltrch  \lang1037\f0\rtlch\'e6\'e5; \'f1\'f2\'e9\'f3 \'e6\'e4 \'ee\'e9\'e5\'f2\'e3 \'e0\'ea \'e5\'f8\'f7 \'ec\'e4\'e2\'ef \'f2\'ec \'f9\'ec\'ee\'e5\'fa \'ee\'f2\'f8\'eb\'fa \'e4\'e4\'f4\'f6\'e4 \'f9\'ec \'e4\'fa\'e5\'eb\'f0\'e4\lang1063\f1\ltrch  \lang1037\f0\rtlch\'e4\'e7\'e5\'f4\'f9\'e9\'fa, \'e4\'ee\'e9\'e5\'f9\'ee\'fa \'e1\'e0\'ee\'f6\'f2\'e5\'fa \'f4\'f8\'f7\'e8\'e9\'f7\'e4 \'f9\'ec \'f8\'f9\'e9\'e5\'f0\'e5\'fa \'f6\'e9\'e1\'e5\'f8\'e9\'e9\'ed. \'e0\'f0\'f9\'e9\'ed\lang1063\f1\ltrch  \lang1037\f0\rtlch\'f8\'e1\'e9\'ed \'e4\'f8\'e9\'ee\'e5 \'fa\'f8\'e5\'ee\'e5\'fa \'f0\'e3\'e9\'e1\'e5\'fa \'ec\'ee\'e2\'e5\'e5\'ef \'f8\'e7\'e1 \'f9\'ec \'fa\'e5\'eb\'f0\'e5\'fa \'e4\'ee\'e5\'f4\'f6\'e5\'fa \'e1\'e0\'ee\'f6\'f2\'e5\'fa\lang1063\f1\ltrch  \lang1037\f0\rtlch\'ee\'f2\'f8\'eb\'fa \'e6\'e5 \'e1\'e4\'f1\'fa\'ee\'ea \'f2\'ec \'e9\'e9\'f9\'e5\'ed \'f2\'f7\'e1\'e9 \'f9\'ec\'e4; \'e6\'e4\'e5 \'fa\'f4\'f7\'e9\'e3\'e5 \'f9\'ec \'e4\'e9\'e5\'f6\'f8/\'e4\'fa\'e5\'f8\'ed\lang1063\f1\ltrch  \lang1037\f0\rtlch\'ec\'e4\'e7\'ec\'e9\'e8 \'e4\'e0\'ed \'e4\'e5\'e0 \'e0\'e5 \'e4\'e9\'e0 \'ee\'e5\'eb\'f0\'e9\'ed \'ec\'e4\'f4\'e9\'f5 \'fa\'e5\'eb\'f0\'e4 \'e1\'e0\'ee\'f6\'f2\'e5\'fa \'ee\'f2\'f8\'eb\'fa \'e0\'e7\'f8\'fa\lang1063\f1\ltrch  \lang1037\f0\rtlch\'eb\'ec\'f9\'e4\'e9 \'e5\'e4\'f8\'e9\'f9\'e9\'e5\'ef \'e0\'e9\'f0\'e5 \'e9\'eb\'e5\'ec \'ec\'eb\'f4\'e5\'fa \'e1\'e7\'e9\'f8\'e4 \'e6\'e5\lang1063\f1\ltrch .\par
