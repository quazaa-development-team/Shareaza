<?xml version="1.0" encoding="UTF-8"?>
    <skin xmlns="http://www.shareaza.com/schemas/Skin.xsd" version="1.0"> 
      <!-- Release 1.2 -->
      <manifest	name="BlueStreak"
			author="RocketX"
			description="An Original Design for all you Shareaza Fans, oh, and it's Blue."
			link="http://shareaza.sf.net/"
			version="1.21"
			type="Skin"
			language="en"
			/>
      <fonts> 
        <font name="Panel.Caption" face="Arial" size="12" weight="bold"/>
	  </fonts>
      <!-- Colour scheme -->
      <colourScheme> 
        <colour name="System.Base.Window"           value="FFFFFF"/>        <!-- main window bg / menu selected hovers -->
        <colour name="System.Base.Midtone"          value="E6E6E6"/>        <!-- main bars bg (toolbars) -->
        <colour name="System.Back.Selected"         value="D9D9D9"/>        <!-- menu / unselected hovers -->
        <colour name="System.Back.Checked.Selected" value="FFFFFF"/>        <!-- menu / checked hovers / mouse downs -->
        <colour name="System.Margin"                value="7C7E7C"/>
        <colour name="Panel.Caption.Text"           value="000000"/>        <!-- area header text colour (if needed) -->
        <colour name="Panel.Caption.Back"           value="000000"/>        <!-- area header bg colour (if needed) -->
        <colour name="TaskPanel.Back"               value="EEEEEE"/>        <!-- side panel bg / search corner bg (E6E6E6)-->
        <colour name="System.Shadow"                value="D6D4D7"/>        <!-- menu icon mouseover hovers -->
        <colour name="System.Border"                value="7C7E7C"/>        <!-- menu hover border colour -->
        <colour name="System.Disabled"              value="7C7E7C"/>        <!-- item inactive/disabled colour -->
        <colour name="System.Text"                  value="000000"/>        <!-- active menu / item text colour -->
        <colour name="Dialog.Back"                  value="E6E6E6"/>        <!-- raza settings bg colour -->
        <colour name="ToolTip.Back"                 value="F7F7F7"/>        <!-- transfers tool tip bg colour -->
        <colour name="ToolTip.Text"                 value="000000"/>        <!-- transfers tool tip text colour -->
        <colour name="TaskBox.Primary.Back"         value="DDDDDD"/>        <!-- side panel (header colour if applic) -->
        <colour name="TaskBox.Primary.Text"         value="000000"/>        <!-- side panel (primary caption colour) -->
        <colour name="TaskBox.Caption.Back"         value="72A1D0"/>        <!-- side panel (border) -->
        <colour name="TaskBox.Caption.Text"         value="000000"/>        <!-- side panel (secondary caption colour) -->
        <colour name="TaskBox.Caption.Hover"        value="333333"/>        <!-- side panel (caption hover colour) -->
        <colour name="TaskBox.Client"               value="FEFEFE"/>        <!-- search panel (bg) ? -->
      </colourScheme>
      <!-- Command bar watermarks -->
      <watermarks> 
        <watermark target="CCoolMenu" path="sts_watermark.bmp"/>
        <watermark target="CSearchPanel" path="sts_sidebg.bmp" />
        <watermark target="CHomeDownloadsBox.Caption" path="sts_bbox.bmp"/>
        <watermark target="CHomeLibraryBox.Caption" path="sts_bbox.bmp"/>
        <watermark target="CHomeConnectionBox.Caption" path="sts_bbox.bmp"/>
        <watermark target="CHomeTorrentsBox.Caption" path="sts_bbox.bmp"/>	<!-- Added as of 2.0.0.13+ -->
        <watermark target="CHomeUploadsBox.Caption" path="sts_bbox.bmp"/>
        <watermark target="CSearchInputBox.Caption" path="sts_bbox.bmp"/>    <!-- search -->
        <watermark target="CSearchResultsBox.Caption" path="sts_bbox.bmp"/>  <!-- results -->
        <watermark target="CSearchSchemaBox.Caption" path="sts_bbox.bmp"/>   <!-- application -->
        <watermark target="CSearchAdvancedBox.Caption" path="sts_bbox.bmp"/>   <!-- advanced panel -->
      </watermarks>
